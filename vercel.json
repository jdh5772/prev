{
    "version": 2,
    "builds": [
      {
        "src": "index.js",			
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/list",
        "dest": "/routes/list.js"
      },
      {
        "src": "/index",
        "dest": "/routes/index.js"
      },
      {
        "src": "/auth",
        "dest": "/routes/auth.js"
      }
    ]
  }